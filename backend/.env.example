# Environment Configuration
NODE_ENV=development
PORT=3001

# API Configuration
API_PREFIX=/api
API_VERSION=v1

# CORS Configuration
CORS_ORIGIN=http://localhost:3000,http://localhost:3001,http://localhost:4200

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined

# CQL Service Configuration
CQL_EXECUTION_SERVICE_URL=http://localhost:8080
CQL_REQUEST_TIMEOUT=30000

# Database Configuration (for future use)
DATABASE_URL=postgresql://postgres:password@localhost:5432/cql_clinic_dev

# Redis Configuration (for future use) 
REDIS_URL=redis://localhost:6379

# JWT Configuration (for future use)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRE=24h

# Security
HELMET_ENABLED=true
COMPRESSION_ENABLED=true

# Monitoring
ENABLE_REQUEST_LOGGING=true
ENABLE_ERROR_LOGGING=true