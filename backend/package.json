{
  "name": "cql-code-clinic-backend",
  "version": "1.0.0",
  "description": "Backend API for CQL Code Clinic - Interactive Clinical Quality Language Learning Platform",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "docker:build": "docker build -t cql-backend .",
    "docker:run": "docker run -p 3001:3001 cql-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "cql",
    "clinical-quality-language",
    "healthcare",
    "learning-platform",
    "api",
    "express",
    "typescript"
